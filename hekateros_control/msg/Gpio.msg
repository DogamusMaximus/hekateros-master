### Hekateros hekateros_control/Gpio message

# Message
uint8 pin       # GPIO pin
uint8 state     # GPIO state or direction

# Pins
uint8 EE_GPIO_1 = 1 # end effector GPIO pin 1
uint8 EE_GPIO_2 = 2 # end effector GPIO pin 2

# States
uint8 OFF = 0       # off (low)
uint8 ON  = 1       # on (high)

# Direction
uint8 IN  = 0       # input
uint8 OUT = 1       # output
